<header class="header">
    <nav class="header__nav">
        <ul class="header__list list">
            {{#each items}}
            <li class="list__item">
                {{> Link class="list__link" href=this.href dataPage=this.dataPage linkName=this.name }}
            </li>
            {{/each}}
        </ul>
    </nav>
</header>